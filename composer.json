{
  "name": "darkanakin41/core-bundle",
  "type": "symfony-bundle",
  "description": "Pierre LEJEUNE CoreBundle",
  "keywords": [
    "Core Bundle"
  ],
  "homepage": "https://github.com/darkanakin41/core-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre LEJEUNE",
      "email": "darkanakin41@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "knplabs/knp-menu-bundle": "^2.0",
    "knplabs/knp-paginator-bundle": "dev-master",
    "symfony/form": "*",
    "symfony/framework-bundle": "*"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Darkanakin41\\CoreBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Darkanakin41\\CoreBundle\\Tests\\": "tests/"
    }
  }
}
