{
  "name": "darkanakin41/core-bundle",
  "type": "symfony-bundle",
  "description": "Pierre LEJEUNE CoreBundle",
  "keywords": [
    "core",
    "symfony",
    "bundle"
  ],
  "homepage": "https://github.com/darkanakin41/core-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre LEJEUNE",
      "email": "darkanakin41@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "*"
  },
  "suggest" : {
    "ext-json": "*",
    "ext-gd": "*",
    "knplabs/knp-menu-bundle": "Because the bundle grant templates form this bundle.",
    "knplabs/knp-paginator-bundle": "Because the bundle grant templates form this bundle.",
    "symfony/form": "Because the bundle grant templates form this bundle."
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "sensiolabs/security-checker": "^6.0",
    "friendsoftwig/twigcs": "^3.2"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Darkanakin41\\CoreBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Darkanakin41\\CoreBundle\\Tests\\": "tests/"
    }
  }
}
